original":{"line":84,"column":0},"generated":{"line":109,"column":41}},{"source":"assets/component/card.vue","name":null,"original":{"line":84,"column":0},"generated":{"line":109,"column":42}},{"source":"assets/component/card.vue","name":null,"original":{"line":84,"column":0},"generated":{"line":110,"column":0}},{"source":"assets/component/card.vue","name":null,"original":{"line":80,"column":0},"generated":{"line":110,"column":7}},{"source":"assets/component/card.vue","name":null,"original":{"line":85,"column":0},"generated":{"line":111,"column":0}},{"source":"assets/component/card.vue","name":null,"original":{"line":61,"column":0},"generated":{"line":112,"column":0}},{"source":"assets/component/card.vue","name":null,"original":{"line":53,"column":0},"generated":{"line":113,"column":0}}],"sources":{"assets/component/card.vue":"<template>\r\n    <section>\r\n        <b-collapse class=\"card\" animation=\"slide\" aria-id=\"contentIdForA11y3\" :open=\"false\" >\r\n            <div\r\n                slot=\"trigger\" \r\n                slot-scope=\"props\"\r\n                class=\"card-header\"\r\n                role=\"button\"\r\n                aria-controls=\"contentIdForA11y3\">\r\n                <p class=\"card-header-title\">\r\n                    {{ task.title }}\r\n                </p>\r\n                <a class=\"card-header-icon\">\r\n                    <b-icon\r\n                        :icon=\"props.open ? 'menu-down' : 'menu-up'\">\r\n                    </b-icon>\r\n                </a>\r\n            </div>\r\n            <div class=\"card-content\">\r\n                <div class=\"content\">\r\n                    {{ task.description }} <a>#{{ this.username }}</a>\r\n                </div>\r\n            </div>\r\n            <footer class=\"card-footer\" v-if=\"task.category === 'backlog'\">\r\n                <a class=\"card-footer-item\" @click=\"deleteTask(task.id)\">Delete</a>\r\n                <a class=\"card-footer-item\" @click=\"goNext(task.id, task.category)\">Next</a>\r\n            </footer>\r\n            <footer class=\"card-footer\" v-if=\"task.category === 'todo'\">\r\n                <a class=\"card-footer-item\" @click=\"goBack(task.id, task.category)\">Back</a>\r\n                <a class=\"card-footer-item\" @click=\"deleteTask(task.id)\">Delete</a>\r\n                <a class=\"card-footer-item\" @click=\"goNext(task.id, task.category)\">Next</a>\r\n            </footer>\r\n            <footer class=\"card-footer\" v-if=\"task.category === 'on-going'\">\r\n                <a class=\"card-footer-item\" @click=\"goBack(task.id, task.category)\">Back</a>\r\n                <a class=\"card-footer-item\" @click=\"deleteTask(task.id)\">Delete</a>\r\n                <a class=\"card-footer-item\" @click=\"goNext(task.id, task.category)\">Next</a>\r\n            </footer>\r\n            <footer class=\"card-footer\" v-if=\"task.category === 'completed'\">\r\n                <a class=\"card-footer-item\" @click=\"goBack(task.id)\">Back</a>\r\n                <a class=\"card-footer-item\" @click=\"deleteTask(task.id)\">Delete</a>\r\n            </footer>\r\n        </b-collapse>\r\n\r\n    </section>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from 'axios'\r\n\r\nconst cats = ['backlog', 'todo', 'on-going', 'completed']\r\n\r\nexport default {\r\n    props: ['task'],\r\n    data() {\r\n        return {\r\n            username: localStorage.getItem('username')\r\n        }\r\n    },\r\n\r\n    methods: {\r\n        goBack(id, cat) {\r\n            axios.put(`http://localhost:3000/task/${id}`, { category: cats[cats.indexOf(cat) - 1] },  { headers: { Authorization: localStorage.getItem('access_token') } })\r\n                .then(result => {\r\n                    this.$emit('refresh-data')\r\n                })\r\n                .catch(error => console.log(error.response))\r\n        },\r\n\r\n        deleteTask(id) {\r\n            axios.delete(`http://localhost:3000/task/${id}`, { headers : { Authorization: localStorage.getItem('access_token') }})\r\n                .then(result => {\r\n                    this.$buefy.snackbar.open('The task already been deleted')\r\n                    this.$emit('refresh-data')\r\n                })\r\n                .catch(error => console.log(error.response))\r\n        },\r\n\r\n        goNext(id, cat) {\r\n            axios.put(`http://localhost:3000/task/${id}`, { category: cats[cats.indexOf(cat) + 1] },  { headers: { Authorization: localStorage.getItem('access_token') } })\r\n                .then(result => {\r\n                    this.$emit('refresh-data')\r\n                })\r\n                .catch(error => console.log(error.response))\r\n        }\r\n    }\r\n}\r\n</script>"},"lineCount":null}},"error":null,"hash":"dd099eb50a0b55e70ea18f695f264a04","cacheData":{"env":{}}}